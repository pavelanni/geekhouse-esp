CompileFlags:
  Remove:
    - -fno-shrink-wrap
    - -fstrict-volatile-bitfields
    - -fno-tree-switch-conversion
    - -fno-jump-tables
    - -fzero-init-padding-bits=all
    - -fno-malloc-dce
    - -mtune=esp-base
    - -specs=picolibc.specs
    - -march=rv32imc_zicsr_zifencei
    - -freorder-blocks
  Add:
    - -march=rv32imc
    - -Wno-unknown-argument
Diagnostics:
  Suppress:
    - unknown_typename
    - builtin_definition
    - redefinition_different_typedef